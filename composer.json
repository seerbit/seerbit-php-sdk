{
  "name": "seerbit/seerbit-php-sdk",
  "description": "SeerBit PHP Library",
  "keywords": ["seerbit", "payment","api"],
  "homepage": "https://seerbit.com",
  "license": ["MIT"],
  "authors": [{
    "name": "SeerBit",
    "email": "developers@seerbit.com",
    "role": "Developer"
  }],
  "require": {
    "php" : "^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "monolog/monolog": ">=1.16"
  },
  "require-dev": {
    "php-coveralls/php-coveralls": "^2.5",
    "squizlabs/php_codesniffer": "^3.7",
    "symfony/process": "~6.2",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {"Seerbit\\": "src/Seerbit"}
  },
  "autoload-dev": {
    "psr-4": {
      "Seerbit\\": [
        "tests/",
        "tests/Seerbit/"
      ]
    }
  }
}
